<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.surovtsev.cool3dminesweeper">

    <application
        android:name="com.surovtsev.cool3dminesweeper.app.Cool3dMinesweeperApplicationImp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_cool_3d_minesweeper"
        android:roundIcon="@mipmap/ic_cool_3d_minesweeper_round"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.CustomSplashScreenTheme">
        <activity
            android:name="com.surovtsev.cool3dminesweeper.presentation.MainActivity"
            android:configChanges="orientation|screenSize|navigation"
            android:exported="true"
            android:screenOrientation="sensor">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>